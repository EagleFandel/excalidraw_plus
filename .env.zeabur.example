PORT=8080
BACKEND_PORT=3005
BACKEND_HOST=127.0.0.1

# Single-container mode (recommended for this repo):
# frontend nginx proxies /api/* to local Nest backend.
API_UPSTREAM=http://127.0.0.1:3005
COLLAB_MAX_SCENE_BYTES=2097152
COLLAB_MAX_FILE_BYTES=4194304
AI_ENABLED=false
AI_PROVIDER=openai-compatible
AI_BASE_URL=https://api.openai.com/v1
AI_API_KEY=
AI_TEXT_TO_DIAGRAM_MODEL=gpt-4.1-mini
AI_DIAGRAM_TO_CODE_MODEL=gpt-4.1-mini
AI_TIMEOUT_MS=45000
